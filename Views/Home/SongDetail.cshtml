@model NoTownMusicalStore.Models.Entities.Song

<div class="movie-detail w-80 m-auto">
    <div class="img-container">
        <img alt="image" src="/Uploads/@Model.SongImage">
    </div>

    <div class="detail-container">
        <h3>@Model.Title</h3>
        <h4>Author : @Model.Author</h4>
        <h4>Album : @Model.Album</h4>
        <h4>Musicians : @Model.Musicians</h4>
        
    </div>
</div>
<div class="w-80 m-auto">
    <a href="/Home/Index" class="btn btn-default">Back</a>
</div>
